{
  "name": "generate-haproxy",
  "instances": 1,
  "merge_logs": true,
  "max_memory_restart": "100M",
  "script": "app.js",
  "log_file": "/var/log/generate-haproxy.log",
  "error_file": "/var/log/generate-haproxy-error.log",
  "out_file": "/var/log/generate-haproxy-out.log",
  "env": {
    "NODE_ENV": "production"
  }
}